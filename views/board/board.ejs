    <%- include('../header') %>
  </head>
  <body>
    <!-- include('../partials/nav')  -->
  
    <div class="container mb-3">

      <h2 class="mb-3"><%=moduleNames[0].module_kr %> 실기 평가 게시판</h2>

      <table class="board-table table table-sm border-bottom">

        <thead class="thead-light">
          <tr>
            <th scope="col">Title</th>
            <th scope="col">name</th>
            <th scope="col" class="date">Date</th>
          </tr>
        </thead>

        <tbody>
          <% if(posts == null || posts.length == 0){ %>
            <tr>
              <td colspan=2> There is no data to show :( </td>
            </tr>
          <% } %>
          <% posts.forEach(function(post,index) { %>
            <tr>
              <td>
                <a href="/board/<%=moduleNames[0].module_eng%>/<%= post.board_id %>">
                  <div class="ellipsis"><%= post.title %></div></a>
              </td>
              <td>
                <span><%= post.name %></span>  
              </td>
              <% if(post.update_at) { %>
                <td class="date">
                    <span data-date="<%= post.update_at %>"></span>
                </td>
              <% } %>
              <% if(post.update_at==null) { %>
                <td class="date">
                    <span data-date="<%= post.create_at %>"></span>
                  </td>
              <% } %>
              
            </tr>
          <% }) %>
        </tbody>

      </table>

      <div>
        <a class="btn btn-primary" href="/board/<%=moduleNames[0].module_eng%>/new">문제 업로드</a>
        <% if(locals.isLogin) { %>
        <a class="btn btn-primary" href="/admin/<%=moduleNames[0].module_eng%>/enterscore">평가 결과 입력</a> 
        <% } %>
        <a class="btn btn-primary" href="/board/<%=moduleNames[0].module_eng%>/selectscore">평가 결과 조회</a>
      </div>

    </div>
    <script src="/js/board.js"></script>
  </body>
</html>